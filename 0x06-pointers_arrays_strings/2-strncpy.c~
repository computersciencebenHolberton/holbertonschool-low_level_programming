#include "holberton.h"

/**
 * *_strncpy - check th
 * @dest: - f1
 * @src: - f2
 * @i:
 * @n: - the use
 * Return: Always 0.
 */
char *_strncpy(char *dest, char *src, int n)
{
  //size_t i;
int i = 0;
for (i = 0; i < n && src[i] != '\0'; i++)
dest[i] = src[i];
for ( ; i < n; i++)
dest[i] = '\0';
return (dest);
}
